{
  "name": "vuelidate",
  "sideEffects": [
    "*.sass",
    "*.scss"
  ],
  "version": "0.7.4",
  "description": "Simple, lightweight model-based validation for Vue.js",
  "author": "Pawe≈Ç Grabarz <pawel.grabarz@monterail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "monterail/vuelidate",
  "scripts": {
    "dev": "node build/dev-server.js",
    "bundle:web": "rimraf dist && cross-env BUILD=web node build/build-bundle.js",
    "bundle:lib": "rimraf lib && cross-env BUILD=lib babel src --out-dir lib",
    "build": "npm run bundle:lib && npm run bundle:web",
    "docs": "rimraf gh-pages && mkdir gh-pages && node build/build-docs.js",
    "unit": "karma start test/unit/karma.conf.js --watch",
    "test-unit": "karma start test/unit/karma.conf.js --single-run",
    "test-ssr": "cross-env BABEL_ENV=node mocha --require @babel/register test/ssr/index.js",
    "test": "npm run test-unit && npm run test-ssr",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.19.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^2.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "node-sass": "^4.12.0",
    "opn": "^6.0.0",
    "ora": "^3.4.0",
    "prettier": "^1.18.2",
    "pug": "2.0.4",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
    "puppeteer": "^1.17.0",
    "raw-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "url-loader": "^2.0.0",
    "vue": "^2.6.10",
    "vue-json-tree-view": "^2.1.4",
    "vue-loader": "^15.7.0",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "validator": "^11.0.0"
  }
}
